<div class="container-interactive" id="containerInteractive">
    <div class="container-interactive-left">
        <span class="container-interactive-left-play">
            <i class="fa-solid fa-play" id="play"></i>
        </span>
        <span class="container-interactive-left-volume">
            <i class="fa-solid fa-volume-high" id="volume"></i>
            <input type="range" name="" id="volumeRange" value="100" />
        </span>
        <span class="container-interactive-left-time">
            <span id="currentTime"></span>
            <span>/</span>
            <span id="entireTime"></span>
        </span>
    </div>
    <div class="container-interactive-right">
        <span>
            <i class="fa-solid fa-expand" id="expand"></i>
        </span>
    </div>
</div>