<div class="videoplayer" id="videoPlayer">
    <div class="videoplayer-top">
        <input type="range" name="" id="timeline" value="0" min="0">
    </div>
    <div class="videoplayer-left">
        <span class="videoplayer-left-play">
            <i class="fa-solid fa-play fa-lg" id="play"></i>
        </span>
        <span class="videoplayer-left-volume">
            <i class="fa-solid fa-volume-high fa-lg" id="volume"></i>
            <input type="range" name="" id="volumeRange" value="100" />
        </span>
        <span class="videoplayer-left-time">
            <span id="currentTime"></span>
            <span class="videoplayer-left-time__slash">/</span>
            <span id="entireTime"></span>
        </span>
    </div>
    <div class="videoplayer-right">
        <span>
            <i class="fa-solid fa-expand fa-lg" id="expand"></i>
        </span>
    </div>
</div>
<script src="/dist/js/videoPlayer.js"></script>